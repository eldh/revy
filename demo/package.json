{
  "name": "revy-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://eldh.github.io/revy",
  "dependencies": {
    "@babel/core": "7.5.4",
    "@glennsl/bs-jest": "^0.4.8",
    "@svgr/webpack": "4.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-emotion": "^10.0.14",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^9.0.0",
    "bfj": "7.0.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "concurrently": "^4.1.1",
    "css-loader": "3.0.0",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "6.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-react": "7.14.2",
    "file-loader": "4.0.0",
    "fs-extra": "8.1.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "inquirer": "^6.5.0",
    "jest": "24.8.0",
    "jest-pnp-resolver": "1.2.1",
    "jest-resolve": "24.8.0",
    "mini-css-extract-plugin": "0.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.8.0",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.0",
    "reason-react": "^0.7.0",
    "resolve": "1.11.1",
    "revy": "*",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "typescript": "^3.5.3",
    "url-loader": "2.0.1",
    "webpack": "4.35.3",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1"
  },
  "scripts": {
    "start": "node scripts/startJs.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "./config/eslintConfig.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {},
  "jest": {
    "collectCoverageFrom": [
      "__tests__/**/*.bs.js",
      "src/**/*Spec.bs.js",
      "src/**/*Test.bs.js",
      "src/**/*_test.bs.js",
      "src/**/*_spec.bs.js"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*.bs.js",
      "<rootDir>/src/**/*(Spec|Test).bs.js",
      "<rootDir>/src/**/*_(test|spec).bs.js"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.js$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.css$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.css$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "emotion"
    ]
  }
}
