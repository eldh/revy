{
  "name": "couloir",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "CouloirApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": ["couloir.lib"],
      "main": "TestCouloir",
      "name": "TestCouloir.exe"
    },
    "library": {
      "name": "couloir.lib",
      "namespace": "Couloir"
    },
    "executable": {
      "require": ["couloir.lib"],
      "main": "CouloirApp",
      "name": "CouloirApp.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestCouloir.exe"
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "^4.4.0",
    "pesy": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
